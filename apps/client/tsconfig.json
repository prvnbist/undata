{
	"extends": "@undata/typescript/nextjs.json",
	"compilerOptions": {
		"baseUrl": "./src",
		"plugins": [
			{
				"name": "next"
			}
		]
	},
	"include": [
		"next-env.d.ts",
		"next.config.js",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts",
		"postcss.config.ts"
	],
	"exclude": ["node_modules"]
}
